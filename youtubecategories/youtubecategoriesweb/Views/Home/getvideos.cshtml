@{
    ViewData["Title"] = "Swathi Priya Annavarapu";
}
@model IEnumerable<YoutubeCategories.Repository.Videos>


<table class="table">
    <thead>
        <tr>
            <th>
                Videos
            </th>
        </tr>
    </thead>
    <tbody>
        @if(Model is null || Model.Count() <= 0){
        <tr>
                <td>
                    Oops! This Region does not have any video in the selected Category.
                </td>
            </tr>
        }
        else{
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <a href="@item.Url">@item.Name</a>
                    </td>
                </tr>
            }
        }
    </tbody>
</table>